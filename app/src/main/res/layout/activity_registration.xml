<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/BimColorBlue"
    tools:context=".RegistrationActivity">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="35dp"
        android:layout_height="47dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="260dp"
        android:src="@drawable/baseline_business_24"
        app:layout_constraintEnd_toEndOf="@+id/orgCodeEditText"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toStartOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/imageView10" />

    <EditText
        android:id="@+id/orgCodeEditText"
        android:layout_width="273dp"
        android:layout_height="51dp"
        android:layout_marginTop="44dp"
        android:hint="@string/organization_code"
        android:inputType="date"
        android:maxLines="1"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword2"
        app:layout_constraintTop_toBottomOf="@+id/emailEditText" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="51dp"
        android:layout_height="71dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/lock_launcher_foreground"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView10"
        app:layout_constraintTop_toTopOf="@+id/imageView10" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="51dp"
        android:layout_height="71dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/email_launcher_foreground"
        app:layout_constraintStart_toStartOf="@+id/imageView9"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="51dp"
        android:layout_height="71dp"
        android:layout_marginTop="48dp"
        android:src="@drawable/date_launcher_foreground"
        app:layout_constraintEnd_toEndOf="@+id/dateOfBirthEditText"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="51dp"
        android:layout_height="71dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/person_launcher_foreground"
        app:layout_constraintEnd_toEndOf="@+id/familyNameEditText"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <Button
        android:id="@+id/registerButton"
        android:layout_width="195dp"
        android:layout_height="44dp"

        android:layout_marginTop="96dp"
        android:text="Register"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword2"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintStart_toEndOf="@+id/imageView11"
        app:layout_constraintTop_toBottomOf="@+id/orgCodeEditText"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="322dp"
        android:layout_height="163dp"
        android:src="@drawable/reglogo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="51dp"
        android:layout_height="71dp"
        android:layout_marginTop="4dp"
        android:src="@drawable/person_launcher_foreground"
        app:layout_constraintEnd_toEndOf="@+id/firstNameEditText"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <EditText
        android:id="@+id/firstNameEditText"
        android:layout_width="272dp"
        android:layout_height="44dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="8dp"
        android:hint="First Name"
        android:inputType="textCapWords"
        android:maxLines="1"
        app:layout_constraintEnd_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <EditText
        android:id="@+id/familyNameEditText"
        android:layout_width="272dp"
        android:layout_height="40dp"
        android:hint="Last Name"
        android:inputType="textCapWords"
        android:maxLines="1"
        app:layout_constraintEnd_toEndOf="@+id/firstNameEditText"
        app:layout_constraintTop_toBottomOf="@+id/firstNameEditText" />

    <EditText
        android:id="@+id/dateOfBirthEditText"
        android:layout_width="273dp"
        android:layout_height="51dp"
        android:hint="Date of Birth (YYYYMMDD)"
        android:inputType="date"
        android:maxLines="1"
        app:layout_constraintEnd_toEndOf="@+id/familyNameEditText"
        app:layout_constraintTop_toBottomOf="@+id/familyNameEditText" />

    <EditText
        android:id="@+id/emailEditText"
        android:layout_width="271dp"
        android:layout_height="45dp"
        android:layout_marginTop="52dp"
        android:hint="@string/Email"
        android:inputType="textEmailAddress"
        android:maxLines="1"
        app:layout_constraintEnd_toEndOf="@+id/dateOfBirthEditText"
        app:layout_constraintTop_toTopOf="@+id/dateOfBirthEditText" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="274dp"
        android:layout_height="45dp"
        android:ems="10"
        android:hint="Enter Password here"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/emailEditText"
        app:layout_constraintTop_toBottomOf="@+id/emailEditText" />

</androidx.constraintlayout.widget.ConstraintLayout>